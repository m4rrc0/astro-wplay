{
	"name": "astro-wplay",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"build:bun": "bun --bun astro build",
		"postbuild": "npm run encrypt && npm run pagefind:build",
		"pagefind:build": "pagefind --site dist",
		"pagefind": "pagefind --site dist --output-path ./public/pagefind",
		"preview": "astro preview",
		"serve": "astro preview",
		"format": "npx prettier -w .",
		"encrypt": "[ -z $PAGE_ADMIN ] || npx staticrypt dist/$PAGE_ADMIN -r -d dist/",
		"encrypt:local": ". ./.env && npx staticrypt dist/$PAGE_ADMIN -r -d dist/"
	},
	"devDependencies": {
		"@astrojs/sitemap": "^3.0.5",
		"@astrojs/svelte": "^5.0.3",
		"@csstools/postcss-design-tokens": "^3.1.1",
		"@directus/sdk": "^16.1.1",
		"@iconify-json/carbon": "^1.1.37",
		"@iconify-json/cib": "^1.1.10",
		"@iconify-json/gridicons": "^1.1.15",
		"@iconify-json/ph": "^1.1.14",
		"@iconify-json/ri": "^1.1.22",
		"@iconify-json/simple-line-icons": "^1.1.9",
		"astro": "^4.13.1",
		"astro-compress": "^2.2.28",
		"astro-critters": "^2.1.10",
		"astro-icon": "^1.1.1",
		"astro-imagetools": "^0.9.0",
		"astro-remote": "^0.2.4",
		"astro-robots-txt": "^1.0.0",
		"cssnano": "^6.0.1",
		"dotenv": "^16.3.1",
		"list.js": "^2.3.1",
		"netlify-plugin-cache-folder": "^0.0.8",
		"pagefind": "^1.1.0",
		"postcss-functions": "^4.0.2",
		"postcss-import": "^15.1.0",
		"postcss-mixins": "^9.0.4",
		"postcss-preset-env": "^9.3.0",
		"prettier": "^3.3.3",
		"prettier-plugin-astro": "^0.14.1",
		"protect-email": "^0.1.2",
		"rrule": "^2.8.1",
		"sharp": "^0.32.6",
		"staticrypt": "^3.3.2"
	}
}
